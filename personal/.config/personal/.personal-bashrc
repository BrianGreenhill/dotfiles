[ -z "$TMUX" ] && export TERM=xterm-256color
[[ -e ~/.config/personal/alias ]] && source ~/.config/personal/alias
[[ -e ~/.config/personal/env ]] && source ~/.config/personal/env

export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8
export PATH=$HOME/bin:$HOME/.local/bin:$GOPATH/bin:/usr/local/sbin::/usr/local/bin:$PATH
export PATH=/usr/local/opt/python/libexec/bin:$PATH
